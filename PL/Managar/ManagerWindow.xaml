<Window x:Class="PL.ManagerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="ManagerWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="OrderButton" Content="view orders" HorizontalAlignment="Center" Height="52" Margin="0,143,0,0" VerticalAlignment="Top" Width="224" FontSize="20" FontFamily="Candara" FontWeight="Bold" Click="OrderButton_Click"/>
        <Button x:Name="ProductButton" Content="view products" HorizontalAlignment="Center" Height="52" VerticalAlignment="Top" Width="224" FontSize="20" FontFamily="Candara" FontWeight="Bold" Margin="0,234,0,0" Click="ProductButton_Click"/>

    </Grid>
</Window>
