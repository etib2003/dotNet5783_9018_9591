<Window x:Class="PL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="tempraryMain" Height="450" Width="800">
    <Grid>
        <Button x:Name="ProductForListButton" Content="Products" Margin="0,54,0,0" VerticalAlignment="Top" FontSize="48" FontFamily="Maiandra GD" Click="ProductButton_Click" FontWeight="Bold" Foreground="#FFFFFBFB" Background="Black" HorizontalAlignment="Center" BorderThickness="0,0,0,0" BorderBrush="#FFEEEEEE" Width="225">
            <Button.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="20"></Setter>
                </Style>
            </Button.Resources>
        </Button>
    </Grid>
</Window>

